{
  "name": "menubar-brightness",
  "description": "My wicked app",
  "productName": "Menubar Brightness",
  "version": "0.0.0",
  "author": {
    "name": "Lucas Bento",
    "email": "lucas.bsilva@outlook.com",
    "url": "github.com/lucasbento"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "icon": "./src/icon.icns",
        "extendInfo": {
          "LSUIElement": 1
        }
      },
      "github_repository": {
        "owner": "lucasbento",
        "name": "menubar-brightness"
      }
    }
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "brightness": "^3.0.0",
    "electron-compile": "^6.4.2",
    "electron-debug": "^1.0.0",
    "electron-osx-appearance": "^0.1.1",
    "electron-settings": "^3.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "menubar": "^5.2.3",
    "nouislider": "^10.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "devtron": "^1.1.0",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.9",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "files": [
    "./src/*"
  ],
  "keywords": [
    "electron",
    "electron-app"
  ],
  "license": "MIT",
  "main": "./src/main.js",
  "repository": "lucasbento/menubar-brightness",
  "scripts": {
    "lint": "eslint .",
    "make": "electron-forge make",
    "package": "electron-forge package --platform=darwin",
    "start": "electron-forge start"
  }
}
